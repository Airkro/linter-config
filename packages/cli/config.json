{
  "*.vue": [
    "prettier --write",
    "eslint --fix",
    "stylelint --fix --rd",
    "git add"
  ],
  "*.{js,jsx,mjs,cjs}": [
    "prettier --write",
    "eslint --fix",
    "git add"
  ],
  "*.{css,scss,less}": [
    "prettier --write",
    "stylelint --fix --rd",
    "git add"
  ],
  "*.{html,tpl,svg,json,toml,yml,yaml,md}": [
    "prettier --write",
    "git add"
  ]
}
